{
  "name": "eigenvue",
  "version": "1.0.2",
  "description": "The visual learning platform for understanding algorithms, AI architectures, and quantum computing.",
  "author": "Ashutosh Mishra <ashutoshm1771@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "bin": {
    "eigenvue": "./dist/cli.js"
  },
  "files": [
    "dist/",
    "data/"
  ],
  "keywords": [
    "visualization",
    "algorithms",
    "deep-learning",
    "transformers",
    "education",
    "binary-search",
    "sorting",
    "attention",
    "step-by-step"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eigenvue/eigenvue",
    "directory": "node"
  },
  "homepage": "https://eigenvue.web.app/",
  "bugs": "https://github.com/eigenvue/eigenvue/issues",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.55.0",
    "vitest": "^1.0.0"
  }
}
