{
  "description": "Apply X then H gates to a single qubit — tests basic gate application",
  "inputs": {
    "numQubits": 1,
    "gates": [
      { "gate": "X", "qubits": [0] },
      { "gate": "H", "qubits": [0] }
    ]
  },
  "expected": {
    "terminalStepId": "apply_gate_1",
    "keyStates": [
      {
        "stepId": "apply_gate_0",
        "amplitudes": [[0, 0], [1, 0]],
        "comment": "X|0⟩ = |1⟩"
      },
      {
        "stepId": "apply_gate_1",
        "amplitudes": [[0.7071067811865476, 0], [-0.7071067811865476, 0]],
        "comment": "H|1⟩ = |−⟩ = (|0⟩ − |1⟩)/√2"
      }
    ]
  }
}
